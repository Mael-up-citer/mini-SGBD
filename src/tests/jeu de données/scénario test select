create database test
set database test


CREATE TABLE Employes (ID_Employe:INT,Salaire:REAL,Nom:char(30),Poste:char(25),Age:INT)
INSERT INTO Employes VALUES (1, 45000.5, "Martin", "Manager", 35)
INSERT INTO Employes VALUES (2, 38000.0, "Dupont", "Analyste", 28)
INSERT INTO Employes VALUES (3, 52000.75, "Durand", "Directeur", 45)
INSERT INTO Employes VALUES (4, 30000.25, "Moreau", "Technicien", 22)
INSERT INTO Employes VALUES (5, 41000.0, "Lemoine", "Comptable", 32)

CREATE TABLE Clients (ID_Client:INT, Nom:char(40), Email:varchar(50), Telephone:varchar(15), Age:INT)

CREATE TABLE Produits (ID_Produit:INT, Nom_Produit:char(50), Prix:REAL, Categorie:char(30), Quantite:INT)
CREATE TABLE Commandes (ID_Commande:INT, ID_Client:INT, Date_Commande:char(20), Montant:REAL, Statut:char(15))
CREATE TABLE Fournisseurs (ID_Fournisseur:INT, Nom_Fournisseur:char(35), Telephone:varchar(20), Adresse:varchar(50), Ville:char(25))

SELECT e.ID_Employe,e.Nom,e.Salaire FROM Employes e WHERE e.Age>30 AND e.Salaire>=50000 AND e.Poste=Manager

SELECT c.Nom,c.Email,c.Telephone
FROM Clients c
WHERE c.Age<40 AND c.Telephone<>0000000000

SELECT p.Nom_Produit,p.Prix,p.Categorie
FROM Produits p
WHERE p.Prix>=100 AND p.Categorie=Electronique AND p.Quantite>0

SELECT f.Nom_Fournisseur,f.Ville,f.Telephone
FROM Fournisseurs f
WHERE f.Ville=Paris AND f.Telephone<>0000000000

SELECT c.ID_Commande,c.Montant,c.Statut
FROM Commandes c
WHERE c.Statut=EnCours AND c.Montant>1000

SELECT p.Nom_Produit,p.Categorie,p.Quantite
FROM Produits p
WHERE p.Quantite<=10 AND p.Categorie=Meubles

SELECT e.ID_Employe,e.Poste,e.Salaire
FROM Employes e
WHERE e.Salaire>30000 AND e.Poste=Développeur

SELECT c.ID_Client,c.Nom,c.Email
FROM Clients c
WHERE c.Nom=John AND c.Email<>NULL

SELECT f.ID_Fournisseur,f.Nom_Fournisseur,f.Adresse
FROM Fournisseurs f
WHERE f.Ville=NewYork AND f.Adresse<>NULL

SELECT c.ID_Commande,c.Montant,c.Statut
FROM Commandes c
WHERE c.Statut=Livré AND c.Montant>=2000



SELECT e.ID_Employe,e.Nom,c.Montant
FROM Employes e,Commandes c
WHERE e.ID_Employe=c.ID_Client AND c.Montant>500

SELECT c.Nom,c.Telephone,p.Nom_Produit,p.Prix
FROM Clients c,Produits p
WHERE c.ID_Client=p.ID_Produit AND p.Categorie=Technologie

SELECT p.Nom_Produit,p.Categorie,f.Nom_Fournisseur,f.Ville
FROM Produits p,Fournisseurs f
WHERE p.ID_Produit=f.ID_Fournisseur AND f.Ville=Berlin

SELECT c.ID_Commande,c.Statut,e.ID_Employe,e.Poste
FROM Commandes c,Employes e
WHERE c.ID_Client=e.ID_Employe AND e.Salaire>40000

SELECT e.ID_Employe,e.Nom,f.Nom_Fournisseur,f.Ville
FROM Employes e,Fournisseurs f
WHERE e.ID_Employe=f.ID_Fournisseur AND e.Age<50

SELECT c.Nom,c.Email,f.Telephone,f.Adresse
FROM Clients c,Fournisseurs f
WHERE c.ID_Client=f.ID_Fournisseur AND f.Telephone<>NULL

SELECT p.Nom_Produit,p.Categorie,e.Nom,e.Poste
FROM Produits p,Employes e
WHERE p.ID_Produit=e.ID_Employe AND p.Quantite<20

SELECT c.ID_Commande,c.Statut,f.Nom_Fournisseur,f.Ville
FROM Commandes c,Fournisseurs f
WHERE c.ID_Commande=f.ID_Fournisseur AND c.Statut=EnCours

SELECT p.Nom_Produit,p.Prix,c.ID_Commande,c.Statut
FROM Produits p,Commandes c
WHERE p.ID_Produit=c.ID_Commande AND c.Statut=EnCours

SELECT e.ID_Employe,e.Salaire,p.Nom_Produit,p.Prix
FROM Employes e,Produits p
WHERE e.ID_Employe=p.ID_Produit AND e.Poste=Manager